{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "!.next/cache/**",
        ".cache",
        ".next/**",
        "app/styles/tailwind.css",
        "build/**",
        "dist/**",
        "public/build/**"
      ]
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "format": {},
    "format:check": {},
    "lint": {},
    "test": {},
    "test:integration": {},
    "typecheck": {}
  }
}
